@using BMCR_projeckt.ViewModels
@model System.Collections.Generic.List<BMCR_projeckt.ViewModels.BuildingViewModel>
@{
    List<BuildingViewModel> buildings = new List<BuildingViewModel>();
    buildings = Model;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">

    <title>Index</title>
</head>
<body>
<p>
    For the other page <a class="btn btn-danger" asp-controller="Create" asp-action="CreateBuilding">click here</a>
</p>

@if (buildings != null)
{
    foreach (BuildingViewModel b in buildings)
    {
        <div>@b.Name</div>
    }
}
<a type="button" class="btn btn-primary" href="/Create/CreateBuilding">
    Create new
</a>
</body>
</html>